  "main": "babel.config.js",
  "scripts": {
    "start": "npm run build && npm run static-server",
    "develop": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "eslint": "eslint \"src/**/*.js\"",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:test": "eslint \"src/**/*.{spec,test}.js\"",
    "prettier": "prettier --check \"src/**/*.{js,css,html,md,json}\" --write",
    "test:jest": "npm run eslint:test && cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "static-server": "static-server ./build",
    "clean": "rimraf build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "file-loader": "^6.0.0",
    "full-icu": "^1.3.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "static-server": "^2.2.1",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.43.0",
